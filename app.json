{
  "expo": {
    "name": "Geosense",
    "slug": "Geosense",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logos/logo-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#020817"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/logo-splash.png",
        "backgroundColor": "#020817"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logos/logo-splash.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
